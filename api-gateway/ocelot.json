{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{catchAll}",
      "DownstreamScheme": "http",
      "ServiceName": "marketplace-service-80",
      "UpstreamPathTemplate": "/marketplace/{catchAll}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/onboarding/{catchAll}",
      "DownstreamScheme": "http",
      "ServiceName": "onboard-service-80",
      "UpstreamPathTemplate": "/onboard/{catchAll}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE"
      ]
    },
    {
      "DownstreamPathTemplate": "/{catchAll}",
      "DownstreamScheme": "ws",
      "ServiceName": "rtm-service-80",
      "UpstreamPathTemplate": "/connect/{catchAll}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE",
        "OPTIONS"
      ]
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}